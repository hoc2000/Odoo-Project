    <template id="report_ticket">
        <t t-call="web.external_layout">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="doc">
                    <h2 class="text-center">Ticket Reports</h2>
                    <span>Ticket Name:</span>
                    <t t-esc="doc.name"/>
                    Customer:
                    <t t-esc="doc.customer_id"/>
                    Assignee:
                    <t t-esc="doc.assign_to"/>
                </t>
            </t>
        </t>
    </template>


</odoo>